<h1>Blogger's New Page</h1>

<%= form_for @blogger do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :age %>
    <%= f.text_field :age %>

    <%= f.label :bio %>
    <%= f.text_area :bio %>

    <%= f.submit %>
<% end %>

<% if flash[:errors] %>
    <% flash[:errors].each do |k, v| %>
        <p><%= v[0]  %></p>
    <%end%> 
<%end%> 


